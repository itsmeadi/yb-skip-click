{
  "manifest_version": 3,
  "name": "AutoClick Extension",
  "version": "1.0",
  "description": "Automatically clicks skip button",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
